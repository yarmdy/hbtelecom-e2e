  <DTS:Executables>
    <DTS:Executable
      DTS:refId="Package\执行 SQL 任务"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="执行 SQL 任务"
      DTS:DTSID="{9B2F941D-4801-4DF9-8765-D7EDBA9C5DCD}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="执行 SQL 任务"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{EF1B0189-C787-45D6-ABB7-2CA7A436C369}"
          SQLTask:SqlStatementSource="BEGIN&#xA;update PDTABLE set STATE = 3 where STATE = 0;&#xA;update PDTABLE set TIMES = TIMES+1 where STATE = 3;&#xA;update PDTABLE p set p.TIMES = 0 where exists(&#xA;select * from DATA_KQIINFO a where TO_CHAR(a.CREATETIME, 'yyyy/MM/dd') =&#xA;TO_CHAR(sysdate-1,'yyyy/MM/dd') AND a.kqiindex &lt;&gt;'暂无' and a.reason&#xA;is not null and p.eci = a.eci and p.TYPE = 'dw' and p.STATE = 3&#xA;);&#xA;update PDTABLE p set p.TIMES = 0 where exists(&#xA;select * from NB_DAY a where TO_CHAR(a.CREATETIME, 'yyyy/MM/dd') =&#xA;TO_CHAR(sysdate-1,'yyyy/MM/dd') AND kqiindex &lt;&gt;'暂无' and reason is not null&#xA; and p.eci = a.eci and p.TYPE = 'nb' and p.STATE = 3&#xA;);&#xA;update PDTABLE set STATE = 1,TIMES=0 where (sysdate-1-createtime&gt;=9 and TYPE='dw') and STATE =3;&#xA;update PDTABLE set STATE = 2,TIMES=0 where (TIMES &gt;= 3 and TYPE='nb' or TIMES &gt;= 3 and TYPE='dw') and STATE = 3;&#xA;delete from UNSOLVED_INFO u where ECGI in (select p.ECI from (SELECT p.ECI,p.STATE,p.TYPE,row_number() over(partition by eci order by CREATETIME desc) top1 FROM PDTABLE p)p where top1=1 and (p.STATE=2) and p.TYPE = u.TYPE );&#xA;insert into PDTABLE select distinct s.*,0,0,'dw' from (select * from (select p.*,row_number()over(partition by eci,type order by createtime desc)top1 from PDTABLE p) where top1=1) p right JOIN (&#xA;  SELECT CREATETIME,&#xA;    SC_ENBID,&#xA;    SC_LCRID,&#xA;    ECI,&#xA;    CITY,&#xA;    MANUFACTOR,&#xA;    CELLNAME,&#xA;    KQIINFO,&#xA;    KQIINDEX,&#xA;    REASON,&#xA;    MEASURES&#xA;  FROM DATA_KQIINFO&#xA;  WHERE TO_CHAR(CREATETIME, 'yyyy/MM/dd') = TO_CHAR(sysdate-1,'yyyy/MM/dd')&#xA;  AND kqiindex &lt;&gt;'暂无'&#xA;  and reason is not null&#xA;  ) s on s.eci = p.eci and p.TYPE = 'dw' where p.eci is null or p.STATE = 2 or p.state=1;&#xA;insert into PDTABLE select distinct s.CREATETIME,s.SC_ENBID,s.SC_LCRID,s.ECI,&#xA;s.CITY,s.MANUFACYOR,s.CELLNAME,s.KQIINFO,s.KQIINDEX,&#xA;s.REASON,s.MEASURES,0,0,'nb' from (select * from (select p.*,row_number()over(partition by eci,type order by createtime desc)top1 from PDTABLE p) where top1=1) p right JOIN (&#xA;SELECT *&#xA;FROM NB_DAY&#xA;WHERE TO_CHAR(CREATETIME, 'yyyy/MM/dd') = TO_CHAR(sysdate-1,'yyyy/MM/dd')&#xA;) s on s.eci = p.eci and p.TYPE = 'nb' where p.eci is null or p.STATE = 2 or p.state=1;&#xA;insert into UNSOLVED_INFO select distinct b.Start_TIme,p.ECI,trunc(0.5 *(decode(b.PAGE_SUM, 0, 0.8, 0.8 * (1 - 1.0 * b.PAGE_NUM / b.PAGE_SUM))&#xA;+ 0.2 * (decode(b.FD_SUM, 0, 1, 1-1.0 * b.FDG_NUM / b.FD_SUM))) + 0.3 *&#xA;(decode(b.VIDEO_SNUM, 0, 0.8, 0.8 * (1-1.0 * b.VIDEO_GNUM / b.VIDEO_SNUM)) + 0.2 *&#xA;(decode(b.VIDEO_SNUM, 0, 1, 1-1.0 * b.VIDEO_BNUM / b.VIDEO_SNUM))) + 0.1 *&#xA;(decode(b.GAME_SUM, 0, 1, 1-1.0 * b.GAME_NUM / b.GAME_SUM)) + 0.1 *&#xA;(decode(b.NEWS_SUM, 0, 1, 1-1.0 * b.NEWS_NUM / b.NEWS_SUM)), 4)&#xA;,'dw' from PDTABLE p JOIN&#xA;KQI_DAY b on p.eci = b.ECGI and p.TYPE = 'dw' and (p.STATE &lt;&gt; 2 or p.state&lt;&gt;1) where TO_CHAR(b.Start_TIme, 'yyyy/MM/dd') = TO_CHAR(sysdate-1,'yyyy/MM/dd');&#xA;insert into UNSOLVED_INFO select distinct n.CREATETIME,p.ECI,n.SUCRATE,'nb' from PDTABLE p JOIN&#xA;  NB_DAY n on p.eci = n.eci and p.TYPE = 'nb' and (p.STATE &lt;&gt; 2 or p.state&lt;&gt;1) where TO_CHAR(n.CREATETIME, 'yyyy/MM/dd') = TO_CHAR(sysdate-1,'yyyy/MM/dd');&#xA;END;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
